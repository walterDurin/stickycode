<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<parent>
		<groupId>net.stickycode.parent</groupId>
		<artifactId>sticky-parent-api8</artifactId>
		<version>1.3</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<groupId>net.stickycode.configured</groupId>
	<artifactId>sticky-configured-content-api</artifactId>
	<version>3.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>sticky-configured-content-api-3</name>
	<description>Taking the configuration of Content out of applications, using the same principles that underly dependency injection</description>
	<inceptionYear>2008</inceptionYear>
	<url>http://www.StickyCode.net/configured/sticky-configured-content-api/</url>

	<scm>
		<connection>scm:svn:http://stickycode.googlecode.com/svn/trunk/net.stickycode.configured/sticky-configured-content-api</connection>
		<developerConnection>scm:svn:https://stickycode.googlecode.com/svn/trunk/net.stickycode.configured/sticky-configured-content-api</developerConnection>
		<url>http://code.google.com/p/stickycode/source/browse/#svn/trunk/net.stickycode.configured/sticky-configured-content-api</url>
	</scm>

	<dependencies>
		<dependency>
			<groupId>net.stickycode.configured</groupId>
			<artifactId>sticky-configured-api</artifactId>
			<version>[5,6)</version>
		</dependency>
		<dependency>
			<groupId>net.stickycode.stereotype</groupId>
			<artifactId>sticky-stereotype-content</artifactId>
			<version>[2,3)</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>clirr-maven-plugin</artifactId>
				<configuration>
					<comparisonVersion>[3.1]</comparisonVersion>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
